<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<property name="CONSOLE_LOG_PATTERN" value="${CONSOLE_LOG_PATTERN:-%date{yyyy.MM.dd HH:mm:ss.SSS} %clr(${LOG_LEVEL_PATTERN:-%5p}) %clr([%thread]){magenta} %clr(%logger{-1}){cyan}->%clr(%method){blue}: %m%n${LOG_EXCEPTION_CONVERSION_WORD:-%wEx}}"/>
	<include resource="org/springframework/boot/logging/logback/base.xml"/>
</configuration>
